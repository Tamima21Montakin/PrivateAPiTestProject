{
  "name": "privateapi-e2e-test",
  "version": "0.1.0",
  "private": true,
  "description": "PrivateAPI E2E Test Cases",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "jest --all --testResultsProcessor ./node_modules/jest-junit -- test/**",
    "build": "tsc",
    "eslint": "eslint -o ./reports/eslint.xml -f checkstyle 'test/**/*.ts' 'src/**/*.ts'",
    "eslint-fix": "eslint --fix 'src/**/*.ts' 'test/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "ssh://gerrit.ps.porters.local/hrbc/test/privateapi-e2e-test"
  },
  "dependencies": {
    "csv-parse": "^5.5.2",
    "fast-xml-parser": "^4.3.2",
    "node-fetch": "^2.6.1",
    "test": "^3.2.1"
  },
  "jest": {
    "collectCoverage": false,
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ],
    "moduleNameMapper": {
      "@util": "<rootDir>/src/util",
      "@dicts": "<rootDir>/src/dicts"
    }
  },
  "jest-junit": {
    "outputDirectory": "reports",
    "outputName": "junit.xml"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.eslint.json"
    }
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node-fetch": "^2.6.2",
    "@types/papaparse": "^5.3.14",
    "ajv": "^8.14.0",
    "dotenv": "^16.4.5",
    "eslint": "^7.32.0",
    "jest": "^28.1.3",
    "jest-junit": "^14.0.1",
    "papaparse": "^5.4.1",
    "prettier": "^2.7.1",
    "punycode": "^2.3.1",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2"
  }
}
